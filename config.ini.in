[system]
binPath = "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/snap/bin"
hashIpCommand = ""
createInterfaces = true
ignoreErrorOnIfCreate = true
allowAllConnections = false
apDetect = false
defaultOpenVlanId = 0
killRunningProcess = true
execAp = true
execRadius = true
execDhcp = true

[capture]
captureInterface = "any"
promiscuous = 0
bufferTimeout = 100

[supervisor]
domainServerPath = /tmp/edgesec-domain-server

[ap]
apBinPath = "./hostapd"
apFilePath = "/tmp/hostapd.conf"
apLogPath = "/tmp/hostapd.log"
interface = "wifiap0"
ssid = "IOTH_IMX7"
wpaPassphrase = "1234554321"
bridge = "br0"
driver = "nl80211"
hwMode = "g"
channel = 11
wmmEnabled = 1
authAlgs = 1
wpa = 2
wpaKeyMgmt = "WPA-PSK"
rsnPairwise = "CCMP"
ctrlInterface = "/var/run/hostapd"
macaddrAcl = 2
dynamicVlan = 1
vlanBridge = "br"
vlanFile = "/tmp/hostapd.vlan"
loggerStdout = -1
loggerStdoutLevel = 0
loggerSyslog = -1
loggerSyslogLevel = 0
ignoreBroadcastSsid = 0
wpaPskRadius = 2

[radius]
port = 1812
clientIP = "127.0.0.1"
clientMask = 32
serverIP = "127.0.0.1"
serverMask = 32
secret = "radius"

[nat]
natInterface = "enp2s0"

[dns]
servers="8.8.4.4,8.8.8.8"

[dhcp]
